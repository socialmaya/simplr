<% if @user and not @group %>
  $("#user_<%= @user.id %>_card_links").remove();
  $("#user_<%= @user.id %>_card_links_anchor").after("<%=j render "users/card_links", user: @user %>");
<% elsif @group %>
  $("#group_<%= @group.id %>_card_links").remove();
  $("#group_<%= @group.id %>_card_links_anchor").after("<%=j render "groups/card_links", group: @group %>");
<% end %>

$("#user_interface_sound")[0].currentTime = 0;
$("#user_interface_sound")[0].play();

window.navigator.vibrate(1);
