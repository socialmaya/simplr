var load_more_notes_link = $("#load_more_notes_link")
var anchor = $("#load_more_notes_anchor");

anchor.after("<%=j render "notes/more_notes", notes: @notes %>");
anchor.remove();

load_more_notes_link.toggle();
$("#load_bar").toggle();

$("#more_content_sound")[0].currentTime = 0;
$("#more_content_sound")[0].play();

<% if @notes.empty? or @notes.size < page_size %>
  load_more_notes_link.remove();
<% end %>

window.navigator.vibrate(1);
