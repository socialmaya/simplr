<% if @home_shown or @preview_shown %>
	<%= render "posts/feed", items: @items, all_items: @all_items %>
<% elsif @proposals %>
  <%= render "proposals/feed", items: @items, all_items: @all_items %>
<% elsif @group_shown %>
	<%= render "groups/feed", items: @items, all_items: @all_items %>
<% end %>
